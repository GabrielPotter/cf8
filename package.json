{
  "name": "cf8",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main/main.js",
  "scripts": {
    "dev:main": "tsc -p tsconfig.main.json -w",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist/main/main.js",
    "dev": "concurrently -k \"npm:dev:main\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "start": "electron --no-sandbox dist/main/main.js"
  },
  "dependencies": {
    "@codemirror/lang-json": "*",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "@uiw/react-codemirror": "^4.23.8",
    "@uiw/codemirror-theme-vscode": "^4.23.8",
    "codemirror": "^6.0.1",
    "electron": "^31.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.4",
    "reactflow": "^11.10.4"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.5.4",
    "vite": "^7.1.2",
    "wait-on": "^8.0.2"
  }
}
